---
import Post from "../components/post.astro";
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const posts = await response.json();
---

<h1>Mis posts</h1>
{
  posts.map((x) => (
    <Post userId={x.userId} title={x.title} body={x.body}/>
  ))
}

<style>
    .title{
        color: chocolate
    }
    body {
      background: #202020;
      color: white;
    }

</style>